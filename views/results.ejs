<% include ./partials/header %>
  	<div class="container resultsWindow d-flex justify-content-center">
  		<div class="card-deck mx-auto">
  			<div class="card displayCard card1">
  				<img src="<%= imageUrl %>" class="card-image-top uploadImage">
  				<div class="card-body">
  					<h5>Your uploaded image.</h5>
  				</div>
  			</div>
  			<div class="card displayCard card2">
  				<img src="<%= results.emoji %>" class="card-image-top emotionImage">
  				<div class="card-body">
  					<h5>Your primary emotion in this image is:</h5>
  					<h4><%= results.emojiText %></h4>
  				</div>
  			</div>
  			<div class="card displayCard card3">
  				<!-- Emotion List -->
	    		<ul class="list-unstyled">
					<li class="media emotion-list">
						<img src="/images/anger.png" class="mr-3 image-parent">
						<div class="media-body align-self-center">
							<div class="progress local-progress">
	  							<div class="progress-bar" role="progressbar" style="max-width: <%= results.emotion.anger %>%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
							</div>
						</div>
					</li>
					<li class="media emotion-list">
						<img src="/images/contempt.png" class="mr-3 image-parent">
						<div class="media-body align-self-center">
